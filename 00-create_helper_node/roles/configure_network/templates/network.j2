<network>
  <name>{{virt_network_name}}</name>
  <forward mode='nat' dev='eth0'>
    <nat>
      <port start='1024' end='65535'/>
    </nat>
  </forward>
  <bridge name='{{virt_network_bridge_name}}' stp='on' delay='0'/>
  <ip address='{{virt_network_ip}}' netmask='{{virt_network_mask}}'>
  </ip>
</network>
